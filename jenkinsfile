 pipeline {
       agent any:
       stages {

       stage  ('Build'){
         steps {
          echo "this is build stage"
          sh 'sleep 5'
         }
     }  
    
       stage ('Deploy'){
         steps {
          echo "this is deploy stage"
          sh 'sleep 5'
         }
     }
 
       stage ('Test'){
         steps {
          echo "this is test stgae"
          sh 'sleep 5'
         }
      }
    }
 }
        
